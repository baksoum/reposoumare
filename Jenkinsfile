pipeline {
    agent any

    stages{   
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage('Soumare') {
            steps {
                echo 'How are you Soumare'
            }
        }

        stage('Bakari') {
            steps {
                echo 'I think you are doing well'
            }
        }
    }
}